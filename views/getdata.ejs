    <body style="background: purple">

        <nav>
            <div class="nav-wrapper" style="background: #7C216F;">
              <a href="#" class="brand-logo right"> 
                  <i class="material-icons large right">add_shopping_cart</i>     Dr. Health Cart</a>
              <ul id="nav-mobile" class="left hide-on-med-and-down">
