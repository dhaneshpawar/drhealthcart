          <h1 style="color:white;" class="header">
              <center>
                Just one step
             </center> </h1> 
          
          <br>
    
      <div class="mycard3">
      
          <div class="row">
              <form class="col s12" id="prospects_form" onsubmit="myfunction()">
                <ul>
                  <li><b>Address : </b> we need your address <b>to deliver you medicine</b>  </li>
                  <li><b>Mobile : </b> we need your mobile number to call you when there is <b>phone call appontment</b> </li>
                </ul>
                <div class="row">
                    <div class="input-field col s6">
                        <input id="first_name" type="text" class="validate" required>
                        <label for="first_name">First Name</label>
                      </div>
                  <div class="input-field col s6">
                    <input id="last_name" type="text" class="validate" required>
                    <label for="last_name">Last Name</label>
                  </div>
                  <div class="input-field col s12">
                      <i class="material-icons prefix">phone</i>
                      <input id="icon_telephone" type="number" class="validate" required>
                      <label for="icon_telephone">Telephone</label>
                    </div>
        
                    <div class="input-field col s12">
                        <textarea id="textarea1" class="materialize-textarea" required></textarea>
                        <label for="textarea1">Local Address</label>
                      </div>

                      
                      <div class="input-field col s6">
                          <input id="landmark" type="text" class="validate" required>
                          <label for="landmark">Landmark</label>
                        </div>
           

                      <div class="input-field col s6">
                          <input id="city_name" type="text" class="validate" required>
                          <label for="city_name">City</label>
                        </div>
           
                      <div class="input-field col s6">
                          <input id="disctrict_name" type="text" class="validate" required>
                          <label for="disctrict_name">Disctrict</label>
                        </div>
           

                        <div class="input-field col s6">
                            <input id="state_name" type="text" class="validate" required>
                            <label for="state_name">State</label>
                          </div>
             
                      <div class="input-field col s6">
                          <input id="country_name" type="text" class="validate" required>
                          <label for="country_name">Country</label>
                        </div>
           
                      
                          
                       
                            <div class="input-field col s6">
                                <input id="pincode_name" type="number" class="validate" required>
                                <label for="pincode_name">Pincode</label>
                              </div>


                                    <div class="input-field col s12">
                                        <textarea maxlength="5000"  rows="4" id="history" placeholder="Explain your age,sex,weight,medical history,current health problems,etc" class="materialize-textarea" required></textarea>
                                        <label for="history">Medical History</label>
                                      </div>
                                 
                                      
                </div>

                Are you agree to our <a href="/privacy_policy">Privacy Policy</a> and <a href="/terms_and_conditions">Terms And Conditions</a></li>
               
                <br>

                <br>
                <center>
                    <button  class="btn green large white-text"  type="submit" name="action">Yes, continiue
                        <i class="material-icons right">send</i>
                      </button>
                
                </center>
                        

              </form>

            </div>
      </div>
      <br><br><br>
       
        <script src="js/jquery-3.2.1.min.js"></script>
        <script src="getdata.js"></script>
      <!--JavaScript at end of body for optimized loading-->
      <script type="text/javascript" src="js/materialize.min.js"></script>

      <script>
      $(document).ready(function() {
        $('select').material_select();
      });
     </script>
       <!--For sign-in with google button-->
      <script>
          function onSuccess(googleUser) {
            console.log('Logged in as: ' + googleUser.getBasicProfile().getName());
          }
          function onFailure(error) {
            console.log(error);
          }
          function renderButton() {
            gapi.signin2.render('my-signin2', {
              'scope': 'profile email',
              'width': 240,
              'height': 50,
              'longtitle': true,
              'theme': 'dark',
              'onsuccess': onSuccess,
              'onfailure': onFailure
            });
          }
        </script>
      
        <script src="https://apis.google.com/js/platform.js?onload=renderButton" async defer></script>
    </body>
